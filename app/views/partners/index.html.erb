<% @page_title = "All Partners" %>
<h1>Partners</h1>
<div class="row">
	<div class="col-md-6">
		<table class="table">
			<thead>
				<th></th>
				<th>Partner</th>
				<th>Level</th>
			</thead>
			<tbody>
			<% @partners.each do |partner| %>
				<tr>
					<td><%= link_to('', edit_partner_path(partner), {data: {toggle: "tooltip"}, title: 'edit', class: "glyphicon glyphicon-edit"}) %><%= link_to('', delete_partner_path(partner), {data: {toggle: "tooltip"}, title: 'delete', class: "glyphicon glyphicon-remove"})%></td>
					<td><%= link_to(partner["name"], partner_path(partner)) %></td>
					<td><%= partner.partner_level.name %></td>
				</tr>
			<% end %>
		<br />
			</tbody>
		</table>
	</div>
</div>
<div class="row"><p><%= link_to('New', new_partner_path)%></p></div>
